<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/cons">


        <ImageView
            android:id="@+id/vehicleImage4"
            android:layout_width="202dp"
            android:layout_height="180dp"
            android:layout_marginStart="2dp"
            android:layout_marginEnd="1dp"
            android:layout_marginBottom="72dp"
            android:src="@drawable/car0"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/vehicleImage3"
            app:layout_constraintTop_toBottomOf="@+id/vehicleImage2"
            app:srcCompat="@color/Black" />

        <ImageView
            android:id="@+id/vehicleImage3"
            android:layout_width="202dp"
            android:layout_height="180dp"
            android:layout_marginEnd="2dp"
            android:layout_marginBottom="72dp"
            android:src="@drawable/car1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/vehicleImage4"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/vehicleImage1"
            app:srcCompat="@color/Black" />

        <ImageView
            android:id="@+id/vehicleImage2"
            android:layout_width="202dp"
            android:layout_height="180dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="43dp"
            android:layout_marginEnd="1dp"
            android:src="@drawable/car2"
            app:layout_constraintBottom_toTopOf="@+id/vehicleImage4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/vehicleImage1"
            app:layout_constraintTop_toBottomOf="@+id/jobinfoCustomer"
            app:srcCompat="@color/Black" />

        <ImageView
            android:id="@+id/vehicleImage1"
            android:layout_width="202dp"
            android:layout_height="180dp"
            android:layout_marginTop="43dp"
            android:layout_marginEnd="2dp"
            android:src="@drawable/car3"
            app:layout_constraintBottom_toTopOf="@+id/vehicleImage3"
            app:layout_constraintEnd_toStartOf="@+id/vehicleImage2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jobinfoCustomer"
            app:srcCompat="@color/Black" />

        <TextView
            android:id="@+id/jobinfoVehicle"
            android:layout_width="371dp"
            android:layout_height="68dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="16dp"
            android:text="Vehicle"
            android:textColor="#000000"
            android:textSize="40dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/jobinfoType"
            android:layout_width="229dp"
            android:layout_height="35dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="161dp"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jobinfoVehicle" />

        <TextView
            android:id="@+id/jobinfoAddress"
            android:layout_width="225dp"
            android:layout_height="36dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="27dp"
            android:layout_marginEnd="165dp"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jobinfoType" />

        <TextView
            android:id="@+id/jobinfoCustomer"
            android:layout_width="225dp"
            android:layout_height="38dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="165dp"
            android:text="TextView"
            android:textColor="#000000"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/jobinfoAddress" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>